{% if attributes -%}
<div{{ attributes }}>
    {% endif %}
    {% if title %}
        <h3>{{ title }}</h3>
    {% endif %}

    {% for row in rows %}
        <div{{ row.attributes.addClass('mansory-grid-item').addClass('col-md-4 pb-4') }}>{{ row.content }}</div>
    {% endfor %}

    {% if attributes -%}
</div>
{% endif %}
